<%= form_for(@user) do |f| %>

  <label>First Name:</label>
  <%= f.text_field :first_name %>

  <label>Last Name:</label>
  <%= f.text_field :last_name %>

  <label>Email Address:</label>
  <%= f.text_field :email %>

  <label>Cell Phone Number:</label>
  <%= f.text_field :phone_number %>

  <%= f.submit %>

<% end %>
