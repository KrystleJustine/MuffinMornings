<h1><%="Edit Info"%> </h1>

<%= form_for @user do |f| %>
  <%= f.label :first_name %>
  <%= f.text_field :first_name %><br><br>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %><br><br>

  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %><br><br>

<%= f.submit "Update Profile"%>
  <% end %>
